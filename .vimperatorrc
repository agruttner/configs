source! /Users/agruttner/.vimperatorrc.local

noremap D d
map d <C-d>
noremap U u
map u <C-u>
noremap J H
noremap K L
map H gT
map L gt
map tn gH
map v ;b
set hlsearch
"set messagetimeout=0
"highlight Hint opacity:0.90
"highlight Hint opacity:0.70
"highlight Hint opacity:0.10

" feedSomeKeys_2.js
autocmd LocationChange .* fmapc
" Gmail
autocmd LocationChange mail\.google\.com/(mail|a)/ :fmap c / j k n p o u e x s l r a # [ ] z ? gi gs gt gd ga gc gl

"Google Reader
autocmd LocationChange www\.google\.com/reader :fmap! -v i j J k K n p m s v A r S N P X O gh ga gs gt gu u / ? t

"qmarks
"==============================================================================
"qmark l dict.leo.org
"qmark m https://mail.google.com/
"qmark r google.com/reader/
"qmark t toodledo.com
"qmark w google.com/a/goindex.com

